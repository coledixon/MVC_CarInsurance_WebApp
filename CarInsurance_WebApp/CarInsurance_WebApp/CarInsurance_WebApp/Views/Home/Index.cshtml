@model IEnumerable<CarInsuranceProjApp.Models.Car>
@{
    ViewBag.Title = "Index";
}
<p>
    @Html.ActionLink("+", "Create", new { id = "" }, new { @class = "btn-floating btn-large waves-effect waves-light" })
</p>
<table class="highlight indigo">
    <thead>
        <tr>
            <th>
                @Html.DisplayName("first")
            </th>
            <th>
                @Html.DisplayName("last")
            </th>
            <th>
                @Html.DisplayName("email")
            </th>
        </tr>
    </thead>
    <tbody class="white">
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.FirstName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.LastName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.EmailAddress)
                </td>
                <td>
                    @Html.ActionLink("Edit", "Edit", new { id = item.Id }, new { @class = "waves-effect waves-teal btn-flat" })
                </td>
            </tr>
        }
    </tbody>
</table>